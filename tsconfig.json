{
  "compilerOptions": {
    "moduleResolution": "node",
    "jsx": "react-native",
    "outDir": "./build",
    "exclude": [
      "node_modules"
    ],
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base"
}
